	    </Text>
            <Appearance>
              <Material/>
            </Appearance>
          </Shape>
          </Transform>
        </Transform>
      </Transform>
      <Transform DEF="cSlider" translation="0 0.1 0">
        <Transform DEF="cTransform" translation="0 0 0.1">
          <PlaneSensor DEF="cSensor" minPosition="0 0" maxPosition="1 0"/>
          <Transform translation="0 0.0 0">
            <TouchSensor DEF="cTS"/>
          </Transform>
          <Transform rotation="0 0 1 0">
          <Shape>
            <Text DEF="cText" string='"c="'>
	     	<FontStyle horizontal="TRUE" size="0.23"/>
	    </Text>
            <Appearance>
              <Material/>
            </Appearance>
          </Shape>
          </Transform>
        </Transform>
      </Transform>
      <Transform DEF="dSlider" translation="0 -0.2 0">
        <Transform DEF="dTransform" translation="0 0 0.1">
          <PlaneSensor DEF="dSensor" minPosition="0 0" maxPosition="1 0"/>
          <Transform translation="0 0.0 0">
